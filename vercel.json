{
  "version": 2,
  "builds": [
    {
      "src": "src/api/**/*.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/meta",
      "dest": "src/api/meta.ts"
    }
  ],
  "functions": {
    "src/api/**/*.ts": {
      "memory": 1024,
      "maxDuration": 60
    }
  },
  "env": {
    "NODE_VERSION": "18"
  }
}
